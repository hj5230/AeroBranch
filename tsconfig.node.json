{
  "extends": "@electron-toolkit/tsconfig/tsconfig.node.json",
  "include": [
    "electron.vite.config.*", "src/main/*", "src/preload/*", "src/shared/**/*"
  ],
  "compilerOptions": {
    "composite": true,
    "types": ["electron-vite/node"],
    "baseUrl": ".",
    "paths": {
      "@main/*": [
        "src/main/*"
      ],
      "@preload/*": [
        "src/preload/*"
      ],
      "@interface/*": [
        "src/shared/interface/*"
      ],
      "@service/*": [
        "src/shared/service/*"
      ],
      "@store/*": [
        "src/shared/store/*"
      ]
    }
  }
}
